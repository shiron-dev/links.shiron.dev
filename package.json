{
  "name": "links-shiron-dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:pages": "pnpm next-on-pages",
    "start": "next start",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint . --fix",
    "pages:build": "pnpm next-on-pages",
    "pages:deploy": "pnpm pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "pnpm next-on-pages --watch",
    "pages:dev": "pnpm wrangler pages dev .vercel/output/static --compatibility-date=2023-12-06 --compatibility-flag=nodejs_compat",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "hono": "^4.6.18",
    "next": "15.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.13.7",
    "@shiron-dev/eslint-config": "2.1.1",
    "@storybook/addon-essentials": "8.5.1",
    "@storybook/addon-interactions": "8.5.1",
    "@storybook/addon-links": "8.5.1",
    "@storybook/addon-onboarding": "8.5.1",
    "@storybook/blocks": "8.5.1",
    "@storybook/nextjs": "8.5.1",
    "@storybook/react": "8.5.1",
    "@storybook/test": "8.5.1",
    "@types/node": "22.10.10",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "eslint": "9.19.0",
    "eslint-plugin-format": "0.1.3",
    "storybook": "8.5.1",
    "typescript": "5.7.3",
    "vercel": "39.3.0",
    "wrangler": "3.105.1"
  }
}
